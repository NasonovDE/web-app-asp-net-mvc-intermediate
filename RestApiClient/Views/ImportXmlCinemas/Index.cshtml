@using RestApiClient.Models
@model ImportXmlCinemaViewModel

<br>
<h3><b>Импорт Кинотеатров XML</b></h3>
<br>

@using (Html.BeginForm("Import", "ImportXmlCinemas", FormMethod.Post, new { @enctype = "multipart/form-data" }))
{
    @Html.EditorForModel()

    @Html.ActionLink("Пример файла", "GetExample", null, new { @class = "btn btn-secondary" })
    <input type="submit" value="Сохранить" class="btn btn-success" />
    @Html.ActionLink("Отменить", "Index", "Cinemas", null, new { @class = "btn btn-danger" })
}